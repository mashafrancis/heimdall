{
  "name": "@heimdall-logs/tracker",
  "version": "0.0.7",
  "description": "A simple tracker for heimdall",
  "main": "./dist/index.js",
  "scripts": {
    "test": "vitest",
    "dev": "rm -rf /dist & tsup --watch",
    "build": "rm -rf /dist & tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "exports": {
    ".": {
      "browser": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "browser": "./dist/react/index.js",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "react": [
        "dist/react/index.d.ts"
      ]
    }
  },
  "types": "dist/index.d.ts",
  "keywords": [
    "heimdall",
    "event tracker",
    "web analytics",
    "analytics"
  ],
  "author": "Francis Masha",
  "license": "MIT",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@vitest/browser": "^2.0.4",
    "axios": "^1.7.2",
    "happy-dom": "^14.12.3",
    "react": "^18.3.1",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4",
    "vitest": "^2.0.4"
  },
  "peerDependencies": {
    "react": "^16.8||^17||^18"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-web": "0.40.0",
    "@opentelemetry/context-zone": "1.25.1",
    "@opentelemetry/core": "1.25.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.52.1",
    "@opentelemetry/instrumentation": "0.52.1",
    "@opentelemetry/instrumentation-document-load": "^0.39.0",
    "@opentelemetry/instrumentation-user-interaction": "^0.39.0",
    "@opentelemetry/instrumentation-xml-http-request": "^0.52.1",
    "@opentelemetry/propagator-b3": "^1.25.1",
    "@opentelemetry/resources": "1.25.1",
    "@opentelemetry/sdk-trace-web": "1.25.1",
    "js-cookie": "^3.0.5",
    "react-error-boundary": "^4.0.13",
    "web-vitals": "^4.2.2"
  }
}
