{
  "name": "@heimdall-logs/tracker",
  "version": "0.0.7",
  "description": "A simple tracker for heimdall",
  "main": "./dist/index.js",
  "scripts": {
    "test": "vitest",
    "dev": "rm -rf /dist & tsup --watch",
    "build": "rm -rf /dist & tsup"
  },
  "exports": {
    ".": {
      "browser": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "browser": "./dist/react/index.js",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": ["dist/index.d.ts"],
      "react": ["dist/react/index.d.ts"]
    }
  },
  "types": "dist/index.d.ts",
  "keywords": ["heimdall", "event tracker", "web analytics", "analytics"],
  "author": "Francis Masha",
  "license": "MIT",
  "files": ["dist", "package.json", "LICENSE", "README.md"],
  "devDependencies": {
    "@heimdall-logs/types": "workspace:*",
    "@types/node": "^20.12.10",
    "@types/react": "^18.3.1",
    "@vitest/browser": "^1.6.0",
    "happy-dom": "^14.10.1",
    "react": "^18.3.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^16.8||^17||^18"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@opentelemetry/api": "1.8.0",
    "@opentelemetry/auto-instrumentations-web": "0.39.0",
    "@opentelemetry/context-zone": "1.24.1",
    "@opentelemetry/core": "1.24.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.51.1",
    "@opentelemetry/instrumentation": "0.51.1",
    "@opentelemetry/propagator-b3": "^1.24.1",
    "@opentelemetry/resources": "1.24.1",
    "@opentelemetry/sdk-trace-base": "1.24.1",
    "@opentelemetry/sdk-trace-web": "1.24.1",
    "@opentelemetry/semantic-conventions": "1.24.1",
    "web-vitals": "^3.5.2"
  }
}
