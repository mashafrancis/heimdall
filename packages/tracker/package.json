{
	"name": "@heimdall-logs/tracker",
	"version": "0.0.7",
	"description": "A simple tracker for heimdall",
	"main": "./dist/index.js",
	"packageManager": "pnpm@8.6.10",
	"scripts": {
		"test": "vitest",
		"dev": "rm -rf /dist & tsup --watch",
		"build": "rm -rf /dist & tsup"
	},
	"exports": {
		".": {
			"browser": "./dist/index.js",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./react": {
			"browser": "./dist/react/index.js",
			"import": "./dist/react/index.js",
			"require": "./dist/react/index.cjs"
		}
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			],
			"react": [
				"dist/react/index.d.ts"
			]
		}
	},
	"types": "dist/index.d.ts",
	"keywords": [
		"heimdall",
		"event tracker",
		"web analytics",
		"analytics"
	],
	"author": "Francis Masha",
	"license": "MIT",
	"files": [
		"dist",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"devDependencies": {
		"@changesets/changelog-git": "^0.2.0",
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.1",
		"@heimdall-logs/types": "0.0.1",
		"@types/jest": "^29.5.11",
		"@types/node": "^20.10.3",
		"@types/react": "^18.2.42",
		"@types/testing-library__jest-dom": "^6.0.0",
		"@vitest/browser": "^1.0.1",
		"happy-dom": "^12.10.3",
		"react": "^18.2.0",
		"sinon": "^17.0.1",
		"terser": "^5.25.0",
		"tsup": "^8.0.1",
		"typescript": "^5.3.2",
		"vitest": "^1.0.1",
		"webdriverio": "^8.24.12"
	},
	"peerDependencies": {
		"react": "^16.8||^17||^18"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@opentelemetry/api": "1.7.0",
		"@opentelemetry/auto-instrumentations-web": "0.34.0",
		"@opentelemetry/context-zone": "1.18.1",
		"@opentelemetry/core": "1.18.1",
		"@opentelemetry/exporter-trace-otlp-http": "0.45.1",
		"@opentelemetry/instrumentation": "0.45.1",
		"@opentelemetry/instrumentation-document-load": "^0.34.0",
		"@opentelemetry/instrumentation-fetch": "^0.45.1",
		"@opentelemetry/instrumentation-user-interaction": "^0.34.0",
		"@opentelemetry/instrumentation-xml-http-request": "^0.45.1",
		"@opentelemetry/propagator-b3": "^1.18.1",
		"@opentelemetry/resources": "1.18.1",
		"@opentelemetry/sdk-trace-base": "1.18.1",
		"@opentelemetry/sdk-trace-web": "1.18.1",
		"@opentelemetry/semantic-conventions": "1.18.1",
		"uuid": "^9.0.1",
		"web-vitals": "^3.5.0"
	}
}
