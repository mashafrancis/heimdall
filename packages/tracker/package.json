{
	"name": "@heimdall-logs/tracker",
	"version": "0.0.3",
	"description": "A simple tracker for heimdall",
	"main": "./dist/index.js",
	"packageManager": "pnpm@8.6.10",
	"scripts": {
		"test": "vitest",
		"dev": "rm -rf /dist & tsup --watch",
		"build": "rm -rf /dist & tsup"
	},
	"exports": {
		".": {
			"browser": "./dist/index.js",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./react": {
			"browser": "./dist/react/index.js",
			"import": "./dist/react/index.js",
			"require": "./dist/react/index.cjs"
		}
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			],
			"react": [
				"dist/react/index.d.ts"
			]
		}
	},
	"types": "dist/index.d.ts",
	"keywords": [
		"heimdall",
		"event tracker",
		"web analytics",
		"analytics"
	],
	"author": "Francis Masha",
	"license": "MIT",
	"files": [
		"dist",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"devDependencies": {
		"@changesets/changelog-git": "^0.1.14",
		"@changesets/changelog-github": "^0.4.8",
		"@changesets/cli": "^2.26.2",
		"@opentelemetry/api": "1.4.1",
		"@opentelemetry/auto-instrumentations-web": "0.31.0",
		"@opentelemetry/core": "1.13.0",
		"@opentelemetry/exporter-trace-otlp-http": "0.34.0",
		"@opentelemetry/instrumentation": "0.34.0",
		"@opentelemetry/sdk-trace-base": "1.13.0",
		"@opentelemetry/sdk-trace-web": "1.8.0",
		"@opentelemetry/semantic-conventions": "1.13.0",
		"@types/jest": "^29.5.5",
		"@types/node": "^20.6.3",
		"@types/react": "^18.2.22",
		"@types/testing-library__jest-dom": "^6.0.0",
		"@vitest/browser": "^0.34.4",
		"happy-dom": "^12.1.2",
		"react": "^18.2.0",
		"sinon": "^16.0.0",
		"terser": "^5.19.4",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2",
		"vitest": "^0.34.4",
		"webdriverio": "^8.16.11"
	},
	"peerDependencies": {
		"react": "^16.8||^17||^18"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@opentelemetry/resources": "1.13.0"
	}
}
