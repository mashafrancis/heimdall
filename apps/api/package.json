	"dependencies": {
		"@clickhouse/client": "^0.2.6",
		"@clickhouse/client-web": "^0.2.6",
		"@heimdall-logs/clickhouse": "0.0.1",
		"@heimdall-logs/db": "0.0.1",
		"@heimdall-logs/logger": "0.0.1",
		"@hono/node-server": "^1.3.1",
		"@libsql/client": "0.4.0-pre.5",
		"@t3-oss/env-core": "^0.7.1",
		"@types/request-ip": "^0.0.41",
		"@types/tar": "^6.1.10",
		"@vercel/kv": "^1.0.0",
		"detect-browser": "^5.3.0",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.29.1",
		"hono": "^3.11.2",
		"isbot": "^3.7.1",
		"jsonwebtoken": "^9.0.2",
		"kafkajs": "^2.2.4",
		"kysely": "^0.26.3",
		"kysely-planetscale": "^1.4.0",
		"maxmind": "^4.3.18",
		"ora": "^7.0.1",
		"request-ip": "^3.3.0",
		"tar": "^6.2.0",
		"tsx": "^4.6.2",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@heimdall-logs/types": "0.0.1",
		"@types/jsonwebtoken": "^9.0.5"
	},
	"exports": {
		".": {
			"import": "./src/index.ts",
			"types": "./src/index.ts"
		},
		"./types": {
			"import": "./src/type.ts",
			"types": "./src/type.ts"
		}
	}
